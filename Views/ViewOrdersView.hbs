
                <div class="ActualContent" style="width: 100%; height: 100%;  display: flex; flex-direction: row; justify-content: center; align-items: center;">
      
                    <div class="searchCont" style="width: 90%; height: 99%;  gap: 9px; background-color: #ebeef1;  display: flex; flex-direction: column; align-items: center;margin-top: 5px;      box-shadow: 10px 10px 10px -1px rgba(10, 99, 169, 0.16),-10px -10px 10px -1px rgba(255, 255, 255, 0.7);">
                    
                    <div class="searbar" style="background-color: #ebeef1;    border-top-right-radius: 5px; border-top-left-radius: 5px;   display: flex; flex-direction: row; justify-content:center; align-items:center;   width: 100%; height: 50px; box-shadow: 10px 10px 10px -1px rgba(10, 99, 169, 0.16),-10px -10px 10px -1px rgba(255, 255, 255, 0.7);">
                        


                      

                             <form action="/commSearchOrder" method="post" style="align-self: center;">
                                <div class="compSearch" style="display: flex; align-items: center;justify-content: center; flex-direction: row; border-right: 2px solid #000; gap: 7px;">
                                <div class="input" style="width: 200px; border-bottom: 3px solid #413e3e; height: 30px;  align-self: center; background-color: #fff; border-radius: 10px;"><input type="text" name="resDate" id="" style="outline: none;  font-weight: bolder; border: 0; width: 200px; height: 30px; border-radius: 10px; text-align: center;" placeholder="Orders Date" required></div>
                                <input type="submit" value="Search" style="width: 100px; cursor: pointer;  height: 30px;  align-self: center; background-color: #000; border-radius: 10px;box-shadow: 10px 10px 10px -1px rgba(10, 99, 169, 0.16), -10px -10px 10px -1px rgba(255, 255,255,0.7);color: white; outline: none; border: 0; ">
                               </div>
                                 </form>


                    
                    </div>

                    <!--Table-->
                    <div class="" style="display: flex; flex-direction:row; gap:15px; justify-content:center;align-items:center;">
                    <div class="tablebox" style="width: 930px; height: 380px; overflow: auto; box-shadow: 10px 10px 10px -1px rgba(10, 99, 169, 0.16), -10px -10px 10px -1px rgba(255, 255,255,0.7);">
                              <table style="width: 100%; " class="table">
                                <thead>
                                    <th>OrderId</th>
                                    <th>Orders</th>
                                    <th>Location</th>
                                     <th>OrderDate</th>
                                         <th>TotalCost</th>
                                            <th>AmountPayed</th>
                                               <th>CustomerId</th>
                                                <th>Telephone</th>
                                                 <th>CustomerName</th>
                                     
                                  
                                  
                                </thead>
                                <tbody>
                                   {{#each  searchResult}}
                                        <tr>
                                            <td data-label="OrderId">{{this.orderId}}</td>
                                          <td data-label="Orders">{{this.Orders}}</td>
                                             <td data-label="Location">{{this.Location}}</td>
                                              <td data-label="OrderDate">{{this.OrderDate}}</td>
                                                <td data-label="TotalCost">{{this.TotalCost}}</td>
                                                  <td data-label="AmountPayed">{{this.AmountPayed}}</td>
                                                    <td data-label="CustomerId">{{this.user_id}}</td>
                                                       <td data-label="Telephone">{{this.Telephone}}</td>
                                                          <td data-label="CustomerName">{{this.customerName}}</td>

                                        </tr> 


                                   {{/each}}
                                  
                                </tbody>
                                <tfoot></tfoot>
                              </table>
                    </div>         
                    <!--Table end-->
                    <div class="box" style="width: 100px; height:100px; border-radius:16px;  background-color:#ebeef1; box-shadow: inset 10px 10px 10px -1px rgba(10, 99, 169, 0.16), inset -10px -10px 10px -1px rgba(255, 255,255,0.7); display:flex; flex-direction:column;align-items:center;justify-content:center; gap:15px">
                        <h5 class="" style="font-weight: bolder;">Orders</h5>
                        <h5 class="" style="font-weight: bolder;">{{this.carCountOnDate}}</h5>

                    </div>
                    </div>

                    </div>     











                </div>